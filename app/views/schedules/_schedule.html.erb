<div id="<%= dom_id schedule %>">
  <p>
    <strong>Title:</strong>
    <%= schedule.title %>
  </p>

  <p>
    <strong>Description:</strong>
    <%= schedule.description %>
  </p>

  <p>
    <strong>Start time:</strong>
    <%= schedule.start_time %>
  </p>

  <p>
    <strong>End time:</strong>
    <%= schedule.end_time %>
  </p>

  <p>
    <strong>Show remaining time:</strong>
    <%= schedule.show_remaining_time %>
  </p>

</div>
